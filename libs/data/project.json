{
  "root": "libs/data",
  "sourceRoot": "libs/data/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nrwl/web:rollup",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "libs/data/dist",
        "tsConfig": "libs/data/tsconfig.lib.json",
        "project": "libs/data/package.json",
        "entryFile": "libs/data/src/index.ts",
        "external": [
          "react/jsx-runtime"
        ],
        "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
        "compiler": "babel",
        "assets": [
          {
            "glob": "libs/data/README.md",
            "input": ".",
            "output": "."
          }
        ],
        "format": [
          "cjs"
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "libs/data/**/*.{ts,tsx,js,jsx}"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/libs/data"
      ],
      "options": {
        "jestConfig": "libs/data/jest.config.js",
        "passWithNoTests": true
      }
    }
  }
}
